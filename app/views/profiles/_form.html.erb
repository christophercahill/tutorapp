
        <%= simple_form_for  @profile, html: {multipart: true} do |f| %>

        <p>
          <%= f.input :name, label: "Name:" %><br>
        </p>
        <p>
          <%= f.input :school, label: "School:" %><br>
        </p>
        <p>
          <%= f.input :year , label: "Year:" %><br>
        </p>
        <p>
          <%= f.input :major, label: "Major:" %><br>
        </p>
        <p>
          <%= f.input :biography, label: "Biography:" %><br>
        </p>
        <p>
         <%= f.button :submit, :class => "form-button"  %>
       </p>
       <% end %>